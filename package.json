{
  "name": "vue-dev-tools-accessibility-ui",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "rm -r -f ./node_modules/.vite && rm -r -f ./node_modules/.vite-temp && npm run dev",
    "dev": "vite --port 5500",
    "build": "vite build",
    "preview": "npm run build && vite preview --port 4173",
    "test": "cross-env TZ=America/New_York vitest run --coverage",
    "unit": "cross-env TZ=America/New_York vitest --coverage",
    "lint": "eslint --ext .js,.vue src tests vite.config.js eslint.config.js",
    "fix": "npm run lint -- --fix",
    "volta": "volta pin node@latest && volta pin npm@latest && git status"
  },
  "ManifestComments": [],
  "dependencies": {
    "color-contrast-picker": "^1.0.8",
    "highlight.js": "^11.11.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.startcase": "^4.4.0",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-doxen": "^0.7.4",
    "vue-options-api-constants-plugin": "^1.0.3",
    "vue-router": "^4.5.1",
    "vue3-snapshot-serializer": "^2.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@pinia/testing": "^1.0.2",
    "@stylistic/eslint-plugin": "^4.4.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-config-tjw-base": "^4.3.0",
    "eslint-config-tjw-import": "^2.0.0",
    "eslint-config-tjw-jest": "^3.0.0",
    "eslint-config-tjw-vue": "^4.0.0",
    "eslint-import-resolver-vite": "^2.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-vue": "^10.2.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "vite": "^6.2.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.44.0"
  },
  "repository": "https://github.com/vue-dev-tools-accessibility/v0",
  "volta": {
    "node": "24.2.0",
    "npm": "11.4.2"
  }
